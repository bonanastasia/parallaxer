    "codeclimate-test-reporter": "^0.5.0",
    "eslint": "^4.1.1",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
